
@model vidly.Models.Movie
@{
    ViewBag.Title = "details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2> @Model.Name</h2>
<p> Date Added: @(Model.DateAdded?.ToString("yyyy-MM-dd") ?? "N/A")</p>
<p> Date Released: @(Model.ReleaseDate?.ToString("yyyy-MM-dd") ?? "N/A") </p>
<p>  Number in Stock : @Model.NumberInStock</p>


